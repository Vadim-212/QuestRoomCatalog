@model IEnumerable<QuestRoomCatalog.Models.QuestsRoomsModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeForQuest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxGamer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinGamer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinAgeGamer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FearLevel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComplexityLevel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeForQuest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxGamer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinGamer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinAgeGamer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FearLevel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ComplexityLevel)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
