@model IEnumerable<QuestRoomCatalog.Models.QuestsRoomsModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateEdit")
</p>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Time for quest</th>
        <th>Max gamer</th>
        <th>Min gamer</th>
        <th>Min age gamer</th>
        <th>Fear level</th>
        <th>Complexity level</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TimeForQuest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxGamer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinGamer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinAgeGamer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FearLevel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ComplexityLevel)
        </td>
        <td>
            @Html.ActionLink("Edit", "CreateEdit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
